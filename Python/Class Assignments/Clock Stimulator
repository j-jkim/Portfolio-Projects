# set up hour
for hour in range(0,24):
   
   # set up minute for every hour
   for minute in range(0,60):
      
      # set up second for every minute
      for second in range(0,60):
         
         # printing out times from midnight to 23:59:59
         print(hour, minute, sep=":",end='')
         print(":"+str(second))
            
         


